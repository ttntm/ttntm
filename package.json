    "date-and-time": "^1.0.0",
    "dotenv": "^16.4.5",
    "eleventy-plugin-reading-time": "0.0.1",
    "eleventy-plugin-toc": "^1.1.5",
    "html-minifier": "^4.0.0",
    "lodash": "^4.17.21",
    "markdown-it-anchor": "^9.0.1",
